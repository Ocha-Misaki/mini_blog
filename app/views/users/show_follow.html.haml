- provide(:title, @title)
.row
  %aside.col-md-4
    .user_info
      %h1= @user.name
      %span= link_to "view my profile", @user
      %span
        %strong Microposts:
        = @user.microposts.count

  .col-md-8
    %h3= @title
    - if @users.any?
      %ul.users.follow
        = render @users
      = will_paginate
